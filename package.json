    "clsx": "^1.2.1",
    "next": "^13.4.1",
    "nprogress": "^0.2.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-markdown": "^8.0.7",
    "react-slick": "^0.29.0",
